# Production Environment Variables

# API Configuration - Railway Backend
VITE_API_URL=https://waypoint-backend-production.railway.app
VITE_NODE_ENV=production

# Frontend URL for Railway backend
VITE_FRONTEND_URL=https://waypoint-lemon.vercel.app

# Stripe Configuration (Production Keys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_...

# Map Configuration
VITE_MAP_TILE_PROVIDER=cartodb-dark
VITE_MAP_ATTRIBUTION="© CartoDB, © OpenStreetMap"

# Analytics and Monitoring
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://...@sentry.io/...

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# Performance
VITE_LAZY_LOADING=true
VITE_CODE_SPLITTING=true

# Security
VITE_CSP_NONCE=auto
VITE_CORS_ORIGIN=https://waypoint.com
